// Primary
@accordionContentPaddingLeft: @inputIconFontSize;

@accordionBackgroundColor: transparent;
@accordionTitleColor: @secondaryColor;
@accordionIcon: "\e907";
@accordionIconFont: 'guideline-icons';

// Border
@togglerBorder: 1px solid;
@togglerBorderColor: #eaecef;
@togglerBorderRadius: 0.25rem;
@togglerPadding: 1rem;
@togglerShadow: 0px 4px 12px rgba(95, 95, 95, 0.153646);